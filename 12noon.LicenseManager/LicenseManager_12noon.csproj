<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <RootNamespace>LicenseManager_12noon</RootNamespace>
    <Nullable>enable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <Platforms>x64</Platforms>
    <PackageIcon />
    <ApplicationIcon>lock.ico</ApplicationIcon>
    <Company>12noon LLC</Company>
    <FileVersion>1.2.4.0</FileVersion>
    <AssemblyVersion>1.2.4</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="lock.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="lock.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CommunityToolkit.Mvvm" Version="8.4.0" />
    <PackageReference Include="Standard.Licensing" Version="1.2.1" />
  </ItemGroup>


  <ItemGroup>
    <Resource Include="lock.png" />
  </ItemGroup>

</Project>
